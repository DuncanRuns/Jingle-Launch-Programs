<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="me.ravalle.programlauncher.gui.ProgramLauncherPanel">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridLayoutManager" row-count="7" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="5" left="5" bottom="5" right="5"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="1661a" class="javax.swing.JLabel" binding="lblTitle">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Right click to add or remove programs"/>
        </properties>
      </component>
      <vspacer id="f819d">
        <constraints>
          <grid row="2" column="1" row-span="2" col-span="2" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
        </constraints>
      </vspacer>
      <component id="8d038" class="javax.swing.JCheckBox" binding="launchProgramsWhenJingleOpens">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Launch programs when Jingle opens"/>
        </properties>
      </component>
      <component id="2e8b4" class="javax.swing.JList" binding="launchPrograms">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="2" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="150" height="50"/>
          </grid>
        </constraints>
        <properties/>
      </component>
      <component id="b5f52" class="javax.swing.JButton" binding="btnLaunchProgsNow">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Launch Programs Now"/>
        </properties>
      </component>
      <component id="5bc7e" class="javax.swing.JCheckBox" binding="launchMCInstance">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Launch Minecraft Instance (requires MultiMC or Prism)"/>
        </properties>
      </component>
      <component id="3b64b" class="javax.swing.JButton" binding="instanceLaunchPath">
        <constraints>
          <grid row="5" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Instance path:"/>
        </properties>
      </component>
      <component id="585d3" class="javax.swing.JButton" binding="launcherExecutablePath">
        <constraints>
          <grid row="6" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Launcher executable:"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
